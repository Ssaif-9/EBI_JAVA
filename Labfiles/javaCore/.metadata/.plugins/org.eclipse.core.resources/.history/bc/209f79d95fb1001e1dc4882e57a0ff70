package core;
public enum Level {
	HIGH, // calls constructor with value 3
	MEDIUM, // calls constructor with value 2
	LOW // calls constructor with value 1
	; // semicolon needed when fields / methods follow

private final int levelCode;
//enum constructor Level
	(int levelCode)
	{
		this.levelCode = levelCode;
	}

// enum Methods
	public int getLevelCode() {
		return this.levelCode1;
	}

}
